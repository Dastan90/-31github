d = [1, 2, 3, 4, 5]
for x in d:
    print(x)

for x in "python":
    print(x)

print(list(range(5))) #[0, 1, 2, 3, 4]

print(list(range(-10, -5)))

print(list(range(-10, -5, 2)))

print(list(range(-10, -5, -2)))

print(list(range(-10, -20, -2)))

d = [1, 2, 3, 4, 5, 6, -11]

p = 1
for i in range(len(d)):
    d[i] = 0

print(d)

S = 0
for i in range(2, 1001):
    S += 1/i

print(S)
